# this is the "smakefile" for SASC6
SCOPTS = UNSCHAR STRMERGE NOSTKCHK PARMS=REGISTER OPTIMIZE
LIBS   = LIB:cgraphx.lib LIB:sc.lib LIB:amiga.lib
SLO    = SC SD ND

GADGETOBJS = classinit.asm.o class_keyboardview_lib.c.o class_keyboardview_dispatch.c.o class_keyboardview_attribs.c.o class_keyboardview_render.c.o class_keyboardview_handleinput.c.o

all: keyboardview.gadget keyboardviewTest keyboardviewTest_static

.asm.o:
	asm $(AFLAGS) $<

.c.o:
	sc $*.c $(SCOPTS) OBJNAME=$@

keyboardview.gadget: $(GADGETOBJS)
	slink $(GADGETOBJS) to $@ lib

#  lib:amiga.lib

clean:
	-delete \#?.(o|lnk)









SCOPTS = UNSCHAR STRMERGE NOSTKCHK PARMS=REGISTER OPTIMIZE

LIBS   = LIB:cgraphx.lib LIB:sc.lib LIB:amiga.lib
SLO    = SC SD ND

.c.o:
	sc $*.c $(SCOPTS) OBJNAME=$@

EXAMPLES = Window ScreenModeReq

all: $(EXAMPLES)

clean:
	-Delete $(EXAMPLES) \#?.(info|o) QUIET

Window: Window.o
	slink FROM LIB:c.o $*.o $(SLO) LIB $(LIBS) TO $@

Window.o: Window.c

ScreenModeReq: ScreenModeReq.o
	slink FROM LIB:c.o $*.o $(SLO) LIB $(LIBS) TO $@

ScreenModeReq.o: ScreenModeReq.c
